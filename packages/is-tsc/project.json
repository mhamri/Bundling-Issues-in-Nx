{
  "name": "tsc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/is-tsc/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/is-tsc",
        "main": "packages/is-tsc/src/index.ts",
        "tsConfig": "packages/is-tsc/tsconfig.lib.json",
        "assets": ["packages/is-tsc/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/is-tsc/**/*.ts",
          "packages/is-tsc/package.json"
        ]
      }
    }
  },
  "tags": []
}
